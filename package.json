{
  "name": "http-request-plus",
  "version": "0.14.0",
  "license": "ISC",
  "description": "Small wrapper around {http,https}.request()",
  "keywords": [
    "http",
    "https",
    "request"
  ],
  "homepage": "https://github.com/JsCommunity/http-request-plus",
  "bugs": "https://github.com/JsCommunity/http-request-plus/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/JsCommunity/http-request-plus"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "bin": {},
  "engines": {
    "node": ">=8.3"
  },
  "dependencies": {
    "is-redirect": "^1.0.0",
    "promise-toolbox": "^0.19.2",
    "pump": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "tap": "^15.1.6"
  },
  "scripts": {
    "dev-test": "tap --no-check-coverage --watch",
    "test": "tap --no-check-coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "\\.spec\\.js$"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --ignore-pattern '!*'"
    ]
  }
}
