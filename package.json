{
  "name": "http-request-plus",
  "version": "0.0.0",
  "license": "ISC",
  "description": "Small wrapper around {http,https}.request()",
  "keywords": [
    "http",
    "https",
    "request"
  ],
  "homepage": "https://github.com/JsCommunity/http-request-plus",
  "bugs": "https://github.com/JsCommunity/http-request-plus/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/JsCommunity/http-request-plus"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "clarify": "^2.0.0",
    "dependency-check": "^2.6.0",
    "ghooks": "^1.3.2",
    "mocha": "^3.0.2",
    "must": "^0.13.2",
    "nyc": "^8.1.0",
    "source-map-support": "^0.4.2",
    "standard": "^8.0.0-beta.5",
    "trace": "^2.3.3"
  },
  "scripts": {
    "build": "NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "depcheck": "dependency-check ./package.json",
    "dev": "babel --watch --source-maps --out-dir=dist/ src/",
    "dev-test": "mocha --opts .mocha.opts --watch --reporter=min \"dist/**/*.spec.js\"",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prepublish": "npm run build",
    "test": "nyc mocha --opts .mocha.opts \"dist/**/*.spec.js\""
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  }
}
