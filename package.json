{
  "name": "http-request-plus",
  "version": "0.0.0",
  "license": "ISC",
  "description": "Small wrapper around {http,https}.request()",
  "keywords": [
    "http",
    "https",
    "request"
  ],
  "homepage": "https://github.com/JsCommunity/http-request-plus",
  "bugs": "https://github.com/JsCommunity/http-request-plus/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/JsCommunity/http-request-plus"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.0.0",
    "babel-plugin-lodash": "^3.2.9",
    "babel-preset-babili": "^0.0.11",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "dependency-check": "^2.6.0",
    "ghooks": "^1.3.2",
    "jest": "^19.0.0",
    "rimraf": "^2.5.4",
    "standard": "^9.0.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "clean": "rimraf dist/",
    "depcheck": "dependency-check ./package.json",
    "predev": "npm run clean",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "dev-test": "jest --watch",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "babel": {
    "env": {
      "production": {
        "comments": false,
        "presets": [
          "babili"
        ]
      }
    },
    "plugins": [
      "lodash"
    ],
    "presets": [
      "latest",
      "stage-0"
    ]
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  },
  "jest": {
    "testPathDirs": [
      "dist/"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  }
}
